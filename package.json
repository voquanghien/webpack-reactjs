{
  "name": "webpack-reactjs",
  "version": "1.0.0",
  "description": "Init ReactJs project with webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --progress --config config/webpack.prod.js",
    "start": "set NODE_ENV=development & rimraf build & webpack --progress --display-modules --config config/webpack.dev.js & webpack-dev-server --config config/webpack.dev.js --content-base dist/ --open",
    "watch": "set NODE_ENV=development & rimraf build & webpack --progress --display-modules --config config/webpack.dev.js --watch"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "7",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.10.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.0",
    "webpack-bundle-size-analyzer": "^3.0.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "nodemon": "^1.18.6",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  }
}
